<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weapes.bookcrawler.mapper.BookMapper">

    <insert id="saveBook">
        INSERT INTO Book(title,author,imageUrl,price,description,
        press,publishDate,amount,type) VALUES(#{title},#{author},
        #{imageUrl},#{price},#{description},#{press},#{publishDate},
        #{amount},#{type})
    </insert>

</mapper>